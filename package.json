{
  "name": "artfulbids",
  "version": "0.0.2",
  "description": "An auction website",
  "type": "module",
  "main": "src/js/pages/main.js",
  "scripts": {
    "test": "npm test",
    "lint": "eslint src/**/*.js",
    "format": "prettier -w src/**/*.js",
    "lint-fix": "eslint src/**/*.js --cache --fix",
    "prepare": "husky install",
    "build": "sass src/scss:css/",
    "watch": "sass --watch src/scss:css/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maleneivy/artfulBids.git"
  },
  "keywords": [
    "website"
  ],
  "author": "Malene Olsen <maleneolsen_@hotmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/maleneivy/artfulBids/issues"
  },
  "homepage": "https://github.com/maleneivy/artfulBids#readme",
  "dependencies": {
    "bootstrap": "^5.3.2"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "live-server": "^1.2.2",
    "prettier": "^3.1.0",
    "sass": "^1.69.5"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.html": [
      "prettier --write"
    ],
    "*.scss": [
      "prettier --write"
    ]
  }
}
